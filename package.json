{
  "name": "synthetix-subgraph",
  "license": "MIT",
  "version": "1.2.1",
  "author": "Synthetix",
  "main": "index.js",
  "browser": "index.min.js",
  "bin": {
    "snxData": "bin.js"
  },
  "scripts": {
    "lint": "eslint .",
    "yaml:chainlink": "node ./create-yaml-file create-yaml --subgraph chainlink --env prod --universal-test-block null | mustache - mustache/yaml.mustache > subgraphs/synthetix-chainlink.yaml",
    "yaml:rates": "node ./create-yaml-file create-yaml --subgraph rates --env prod --universal-test-block null | mustache - mustache/yaml.mustache > subgraphs/synthetix-rates.yaml",
    "yaml:exchanger": "node ./create-yaml-file create-yaml --subgraph exchanger --env prod --universal-test-block null | mustache - mustache/yaml.mustache > subgraphs/synthetix-exchanger.yaml",
    "codegen": "node prepare-abis && graph codegen",
    "codegen:chainlink": "npm run yaml:chainlink && npm run codegen -- subgraphs/synthetix-chainlink.yaml",
    "codegen:snx": "npm run codegen -- subgraphs/synthetix.yaml",
    "codegen:rates": "npm run yaml:rates && npm run codegen -- subgraphs/synthetix-rates.yaml",
    "codegen:depot": "npm run codegen -- subgraphs/synthetix-depot.yaml",
    "codegen:exchanges": "npm run codegen -- subgraphs/synthetix-exchanges.yaml",
    "codegen:loans": "npm run codegen -- subgraphs/synthetix-loans.yaml",
    "codegen:options": "npm run codegen -- subgraphs/synthetix-binary-options.yaml",
    "codegen:orders": "npm run codegen -- subgraphs/synthetix-limit-orders.yaml",
    "codegen:exchanger": "npm run yaml:exchanger && npm run codegen -- subgraphs/synthetix-exchanger.yaml",
    "codegen:grantsdao": "npm run codegen -- subgraphs/synthetix-grantsdao.yaml",
    "codegen:liquidations": "npm run codegen -- subgraphs/synthetix-liquidations.yaml",
    "codegen:shorts": "npm run codegen -- subgraphs/synthetix-shorts.yaml",
    "codegen:collateral-erc20-loans": "npm run codegen -- subgraphs/synthetix-collateral-erc20-loans.yaml",
    "codegen:collateral-eth-loans": "npm run codegen -- subgraphs/synthetix-collateral-eth-loans.yaml",
    "codegen:shorts:kovan": "npm run codegen -- subgraphs/synthetix-shorts-kovan.yaml",
    "codegen:collateral-erc20-loans:kovan": "npm run codegen -- subgraphs/synthetix-collateral-erc20-loans-kovan.yaml",
    "codegen:collateral-eth-loans:kovan": "npm run codegen -- subgraphs/synthetix-collateral-eth-loans-kovan.yaml",
    "build": "graph build",
    "build:snx": "graph build subgraphs/synthetix.yaml",
    "build:chainlink": "graph build subgraphs/synthetix-chainlink.yaml",
    "build:rates": "graph build subgraphs/synthetix-rates.yaml",
    "build:depot": "graph build subgraphs/synthetix-depot.yaml",
    "build:exchanges": "graph build subgraphs/synthetix-exchanges.yaml",
    "build:loans": "graph build subgraphs/synthetix-loans.yaml",
    "build:options": "graph build subgraphs/synthetix-binary-options.yaml",
    "build:orders": "graph build subgraphs/synthetix-limit-orders.yaml",
    "build:exchanger": "graph build subgraphs/synthetix-exchanger.yaml",
    "build:grantsdao": "graph build subgraphs/synthetix-grantsdao.yaml",
    "build:liquidations": "graph build subgraphs/synthetix-liquidations.yaml",
    "build:shorts": "graph build subgraphs/synthetix-shorts.yaml",
    "build:collateral-erc20-loans": "graph build subgraphs/synthetix-collateral-erc20-loans.yaml",
    "build:collateral-eth-loans": "graph build subgraphs/synthetix-collateral-eth-loans.yaml",
    "build:shorts:kovan": "graph build subgraphs/synthetix-shorts-kovan.yaml",
    "build:collateral-erc20-loans:kovan": "graph build subgraphs/synthetix-collateral-erc20-loans-kovan.yaml",
    "build:collateral-eth-loans:kovan": "graph build subgraphs/synthetix-collateral-eth-loans-kovan.yaml",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ --access-token $THEGRAPH_SNX_ACCESS_TOKEN",
    "deploy:snx": "npm run deploy -- synthetixio-team/synthetix subgraphs/synthetix.yaml",
    "deploy:chainlink": "npm run deploy -- synthetixio-team/synthetix-chainlink subgraphs/synthetix-chainlink.yaml",
    "deploy:rates": "npm run deploy -- synthetixio-team/synthetix-rates subgraphs/synthetix-rates.yaml",
    "deploy:depot": "npm run deploy -- synthetixio-team/synthetix-depot subgraphs/synthetix-depot.yaml",
    "deploy:exchanges": "npm run deploy -- synthetixio-team/synthetix-exchanges subgraphs/synthetix-exchanges.yaml",
    "deploy:loans": "npm run deploy -- synthetixio-team/synthetix-loans subgraphs/synthetix-loans.yaml",
    "deploy:options": "npm run deploy -- synthetixio-team/synthetix-binary-options subgraphs/synthetix-binary-options.yaml",
    "deploy:orders": "npm run deploy -- synthetixio-team/synthetix-limit-orders subgraphs/synthetix-limit-orders.yaml",
    "deploy:exchanger": "npm run deploy -- synthetixio-team/synthetix-exchanger subgraphs/synthetix-exchanger.yaml",
    "deploy:grantsdao": "npm run deploy -- synthetixio-team/synthetix-grantsdao subgraphs/synthetix-grantsdao.yaml",
    "deploy:liquidations": "npm run deploy -- synthetixio-team/synthetix-liquidations subgraphs/synthetix-liquidations.yaml",
    "deploy:shorts": "npm run deploy -- vbstreetz/synthetix-shorts subgraphs/synthetix-shorts.yaml",
    "deploy:collateral-erc20-loans": "npm run deploy -- vbstreetz/collateral-erc20-loans subgraphs/synthetix-collateral-erc20-loans.yaml",
    "deploy:collateral-eth-loans": "npm run deploy -- vbstreetz/collateral-eth-loans subgraphs/synthetix-collateral-eth-loans.yaml",
    "deploy:shorts:kovan": "npm run deploy -- vbstreetz/synthetix-shorts-kovan subgraphs/synthetix-shorts-kovan.yaml",
    "deploy:collateral-erc20-loans:kovan": "npm run deploy -- vbstreetz/collateral-erc20-loans-kovan subgraphs/synthetix-collateral-erc20-loans-kovan.yaml",
    "deploy:collateral-eth-loans:kovan": "npm run deploy -- vbstreetz/collateral-eth-loans-kovan subgraphs/synthetix-collateral-eth-loans-kovan.yaml",

    "codegen:delegate-approvals": "npm run codegen -- subgraphs/synthetix-delegate-approvals.yaml",
    "codegen:delegate-approvals:kovan": "npm run codegen -- subgraphs/synthetix-delegate-approvals-kovan.yaml",
    "build:delegate-approvals": "graph build subgraphs/synthetix-delegate-approvals.yaml",
    "build:delegate-approvals:kovan": "graph build subgraphs/synthetix-delegate-approvals-kovan.yaml",
    "deploy:delegate-approvals": "npm run deploy -- vbstreetz/delegate-approvals subgraphs/synthetix-delegate-approvals.yaml",
    "deploy:delegate-approvals:kovan": "npm run deploy -- vbstreetz/delegate-approvals-kovan subgraphs/synthetix-delegate-approvals-kovan.yaml",


    "create-local": "graph create --node http://localhost:8020/ synthetixio-team/synthetix",
    "remove-local": "graph remove --node http://localhost:8020/ synthetixio-team/synthetix",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 synthetixio-team/synthetix"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "git://github.com/graphprotocol/graph-cli.git#ebeeba0009f026bacf940f25ee7b875fe7676032",
    "@graphprotocol/graph-ts": "0.18.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "env-cmd": "^10.1.0",
    "mustache": "4.0.1",
    "synthetix": "2.37.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "minify": "4.1.3",
    "prettier": "1.17.1",
    "typescript": "3.8.3"
  }
}
